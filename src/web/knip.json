{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/App.tsx",
    "orval.config.ts",
    "vite.config.{js,ts,mjs,cjs,mts}",
    "eslint.config.{js,ts,mjs,cjs,mts}"
  ],
  "project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}!"],
  "ignore": [
    "src/api/**",
    "src/pages/children/IndexChildPage.tsx",
    "src/pages/children/NewChildPage.tsx",
    "src/pages/children/UpdateChildPage.tsx",
    "src/pages/groups/ListGroupsPage.tsx",
    "src/pages/people/IndexPersonPage.tsx",
    "src/pages/ScheduleOverviewPage.tsx",
    "src/pages/settings/ClosurePeriodsSettingsPage.tsx",
    "src/pages/settings/SchedulingSettingsPage.tsx",
    "src/pages/settings/SettingsPage.tsx"
  ],
  "paths": {
    "@/*": ["src/*"],
    "@api/*": ["src/api/*"],
    "@providers/*": ["src/providers/*"],
    "@components/*": ["src/components/*"],
    "@pages/*": ["src/pages/*"],
    "@features/*": ["src/features/*"],
    "@hooks/*": ["src/hooks/*"],
    "@lib/*": ["src/lib/*"],
    "@utils/*": ["src/utils/*"]
  },
  "typescript": {
    "config": ["tsconfig.app.json", "tsconfig.json", "tsconfig.node.json"]
  },
  "vite": {
    "entry": ["src/index.tsx"],
    "config": ["vite.config.{js,ts,mjs,cjs,mts}"]
  },
  "eslint": {
    "config": ["eslint.config.{js,ts,mjs,cjs,mts}"]
  }
}